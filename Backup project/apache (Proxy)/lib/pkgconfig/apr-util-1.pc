prefix=/opt/apache
exec_prefix=/opt/apache
libdir=${prefix}/lib
APRUTIL_MAJOR_VERSION=1
includedir=/opt/apache/include

Name: APR Utils
Description: Companion library for APR
Version: 1.5.4
# assume that apr-util requires libapr of same major version
Requires: apr-1
Libs: -L${libdir} -laprutil-${APRUTIL_MAJOR_VERSION}  /root/httpd-2.2.34/srclib/apr-util/xml/expat/libexpat.la
Cflags: -I${includedir}
